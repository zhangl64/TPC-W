         <query>
         	<query-method>
            	<method-name>findByTitle</method-name>
            	<method-params>
               		<method-param>java.lang.String</method-param>
               		<method-param>int</method-param>
               	</method-params>
            </query-method>
            <jboss-ql><![CDATA[
               SELECT OBJECT(I)
               FROM item I
               WHERE I.I_TITLE LIKE ?1 
               ORDER BY I.I_TITLE ASC LIMIT ?2
            ]]></jboss-ql>
         </query>
         
         <query>
         	<query-method>
            	<method-name>findBySubject</method-name>
            	<method-params>
               		<method-param>java.lang.String</method-param>
               		<method-param>int</method-param>
               	</method-params>
            </query-method>
            <jboss-ql><![CDATA[
               SELECT OBJECT(I)
               FROM item I
               WHERE I.I_SUBJECT = ?1
               ORDER BY I.I_TITLE ASC LIMIT ?2
            ]]></jboss-ql>
         </query>
         
         <query>
         	<query-method>
            	<method-name>findNewBySubject</method-name>
            	<method-params>
               		<method-param>java.lang.String</method-param>
               		<method-param>int</method-param>
               	</method-params>
            </query-method>
            <jboss-ql><![CDATA[
               SELECT OBJECT(I)
               FROM item I
               WHERE I.I_SUBJECT = ?1
               ORDER BY I.I_PUB_DATE DESC LIMIT ?2
            ]]></jboss-ql>
         </query>
